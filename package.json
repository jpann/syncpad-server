{
  "name": "syncpad-server",
  "description": "Server for a somewhat collaborative web-based text editor that uses encryption.",
  "version": "0.2.8",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "set DEBUG=syncpad-server:* & node ./bin/www",
    "prod": "gulp uglify && node ./bin/www",
    "update:packages": "node wipe-dependencies.js && rm -rf node_modules && npm update --save-dev && npm update --save"
  },
  "author": "jpann",
  "license": "MIT",
  "dependencies": {
    "body-parser": "*",
    "connect-ensure-login": "*",
    "connect-flash": "*",
    "connect-sqlite3": "*",
    "cookie-parser": "*",
    "crypto-js": "^3.1.8",
    "express": "*",
    "express-session": "*",
    "express-validator": "^3.2.0",
    "guid": "0.0.12",
    "human-readable-ids": "*",
    "moment": "*",
    "moniker": "*",
    "morgan": "*",
    "passport": "^0.4.0",
    "passport-local": "*",
    "password-hash": "*",
    "rotating-file-stream": "*",
    "sanitize-html": "*",
    "shortid": "*",
    "socket.io": "^3.1.2",
    "sqlite": "^3.0.3",
    "underscore": "*"
  },
  "devDependencies": {
    "gulp": "^4.0.2",
    "gulp-uglify": "^3.0.2",
    "pump": "^1.0.2"
  }
}
